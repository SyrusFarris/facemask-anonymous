<section>
    {{#if reviews.length}}
    <div class="new-title">
        <h1>Newest Reviews</h1>
    </div>
    <div class="d-flex flex-row new-reviews">
        {{#each reviews as |review|}}
        <section>
            {{> review-info review}}
        </section>
        {{/each}}
    </div>
    {{/if}}
</section>